{
	"inith": {
		"Cflags": [
			"-g",
			"-Wall",
			"-Wno-missing-braces",
			"-Wno-parentheses",
			"-Wno-unknown-pragmas",
			"-O0",
			"-static",
			"-fplan9-extensions",
			"-ffreestanding",
			"-fno-builtin",
			"-Tinit.ld",
			"-mcmodel=small"
		],
		"Libs": [
			"/arch/$ARCH/lib/libjehanne.a"
		],
		"Post": [
			"export TOOL=strip && $TOOLPREFIX$TOOL init",
			"elf2c init > init.h"
		],
		"Program": "init",
		"SourceFiles": [
			"init9.c",
			"../port/initcode.c"
		]
	}
}
