void decaf_neg(mpint *p, mpint *n, mpint *r){
	mpint *m = mpnew(0);
	mpmodsub(mpzero, r, p, m);
	mpint *tmp1 = mpnew(0);
	mpsub(p, mpone, tmp1);
	mpright(tmp1, 1, tmp1);
	mpsel(-mpcmp(n, tmp1) >> (sizeof(int)*8-1), m, r, r);
	mpfree(tmp1);
	mpfree(m);
	}
